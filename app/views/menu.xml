<Alloy>
	<TableView id="menu" class="container">
		<TableViewSection>
			<TableViewRow id="attention">
				<Widget class="icon iconleft" icon="fa-bullhorn" src="be.k0suke.fontawesome"/>
				<Label class="title">注目</Label>
				<Widget class="icon iconright iconarrow" icon="fa-chevron-right" src="be.k0suke.fontawesome"/>
				<View class="separator"/>
			</TableViewRow>
			<TableViewRow id="column" class="lockrow">
				<Widget class="icon iconleft lockcolor" icon="fa-file-text-o" src="be.k0suke.fontawesome"/>
				<Label class="title lockcolor">コラム</Label>
				<Widget class="icon iconright" icon="fa-lock" src="be.k0suke.fontawesome"/>
				<View class="separator"/>
			</TableViewRow>
			<TableViewRow id="favorite" class="lockrow">
