			<TableViewRow id="setting">
				<Widget class="icon iconleft" icon="fa-cog" src="be.k0suke.fontawesome"/>
				<Label class="title">設定</Label>
				<Widget class="icon iconright iconarrow" icon="fa-chevron-right" src="be.k0suke.fontawesome"/>
				<View class="separator"/>
